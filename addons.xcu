<?xml version="1.0" encoding="UTF-8"?>
<oor:component-data
      xmlns:oor="http://openoffice.org/2001/registry"
      xmlns:xs="http://www.w3.org/2001/XMLSchema"
      oor:package="org.openoffice.Office"
      oor:name="Addons">

    <node oor:name="AddonUI">
        <node oor:name="OfficeMenuBar">
            <node oor:name="org.javelot" oor:op="replace">
                <prop oor:name="Title" oor:type="xs:string">
                    <value>Javelot</value>
                </prop>

                <node oor:name="Submenu">
                    <node oor:name="configure" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?configure</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Configurer</value>
                            <value xml:lang="en-US">Configure</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>

                    <node oor:name="print" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?print</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Imprimer</value>
                            <value xml:lang="en-US">Print</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>
                    
                    <node oor:name="updateGantt" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?updateGantt</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Mettre à jour le Gantt</value>
                            <value xml:lang="en-US">Update the Gantt diagram</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>
                    
                    <node oor:name="updateBudgetTable" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?updateBudgetTable</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Mettre à jour la table du budget</value>
                            <value xml:lang="en-US">Update the budget table</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>
                    
                    <node oor:name="updateFeatureTable" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?updateFeatureTable</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Mettre à jour la table des fonctionnalités</value>
                            <value xml:lang="en-US">Update the feature table</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>
 
                    <node oor:name="updateMethodologyTable" oor:op="replace">
                        <prop oor:name="URL" oor:type="xs:string">
                            <value>service:org.javelot.Main?updateMethodologyTable</value>
                        </prop>
                        <prop oor:name="Title" oor:type="xs:string">
                            <value/>
                            <value xml:lang="fr">Mettre à jour la méthodologie</value>
                            <value xml:lang="en-US">Update the methodology</value>
                        </prop>
                        <prop oor:name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                        <prop oor:name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                    </node>
                </node>
            </node>
        </node>
    </node>
</oor:component-data>
